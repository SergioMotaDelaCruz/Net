@section scripts{

    <script src="~/Scripts/grafica/highstock.js"></script>
    <script src="~/Scripts/grafica/exporting.js"></script>
    <script>

        var mes_seleccionado;
        var anio_seleccionado;

        function mes(valor) {
            switch (valor) {
                case "enero":
                    resultado = "01";
                    break;
                case "febrero":
                    resultado = "02";
                    break;
                case "marzo":
                    resultado = "03";
                    break;
                case "abril":
                    resultado = "04";
                    break;
                case "mayo":
                    resultado = "05";
                    break;
                case "junio":
                    resultado = "06";
                    break;
                case "julio":
                    resultado = "07";
                    break;
                case "agosto":
                    resultado = "08";
                    break;
                case "septiembre":
                    resultado = "09";
                    break;
                case "octubre":
                    resultado = "10";
                    break;
                case "noviembre":
                    resultado = "11";
                    break;
                case "diciembre":
                    resultado = "12";
                    break;
                default:
                    resultado = "12";
            }


            mes_seleccionado = resultado;

        }

        function anio(valor) {
            anio_seleccionado = valor;
        }

		function reset(){
			var elemento = document.getElementById("select_mes");
			elemento.selectedIndex = 0;
			elemento = document.getElementById("select_anio");
			elemento.selectedIndex = 0;

		}

        function descargar() {
            reset();
            var temp = "";
            temp = anio_seleccionado.substring(2, 4);
            var cadena = resultado + temp;

            if (cadena == "0504" || cadena == "0404" || cadena == "0304" || cadena == "0204" || cadena == "0104") {
                alert("La consulta de los documentos debe ser como mínimo Junio de 2004.");
            } else {
                window.open("../docs/historicos/espana/" + anio_seleccionado + "/" + mes_seleccionado + temp + " - Cartesio Equity.pdf");
            }
        }




        verGrafico("_grafica_cartesio_equity", null, null);

            function verGrafico(urlbtn, fechaIni, fechaFin) {

                var url = "/Graficos/" + urlbtn + "?_=" + $.now() +"&fechaIni=" + fechaIni + "&fechaFin=" + fechaFin + "&vista=admin";

                $.ajax({
                    cache: false,
                    url: url,
                    type: "get",
                    success: function (data) {
                        $("#vistaGrafico").html(data);
                        $(".datepicker").datepicker("option", "dateFormat", "dd/mm/yy");

                    },
                    error: function (xhr, ajaxOptions, thrownError) {

                    }
                });
            }

            $("#historico").click(function () {
                $("#informes_anteriores").toggle('fade', 500);
            });
            $(function () {
                $('#menu').data('size', 'big');
            });
            $(document.body).mousedown(function (event) {
                var target = $(event.target);
                if (!target.parents().andSelf().is('#informes_anteriores')) { // Clicked outside
                    $('#informes_anteriores').fadeOut(500);
                }
            });

    </script>
}




